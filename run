#!/bin/bash

cd ~/thesis/SWE/build
rm  precice-Solver* reference.xml

size=300
domain1="Gtest1_unidirectional"
domain2="Gtest2_unidirectional"

rm  precice-Solver* reference.xml output/$domain1* output/$domain2*

if  [ $1 == "1" ];then
  ./SWE_gnu_debug_none_fwave_1 -x $size -y $size -o output/$domain1
elif [ $1 == "2" ]; then
  ./SWE_gnu_debug_none_fwave_2 -x $size -y $size -o output/$domain2
elif [ $1 == "b" ]; then
   ./SWE_gnu_debug_none_fwave_1 -x $size -y $size -o output/$domain1
   ./SWE_gnu_debug_none_fwave_2 -x $size -y $size -o output/$domain2
else
  echo "not valid option"
fi
 cd ..
